entor-widget-container\">\n\t\t\t<script>\nconst getMarqueeNode = content => {\nconst el = document.createElement('div');\nel.setAttribute('data-marquee-style', true);\nel.textContent = content\nreturn el;\n}\nclass Marquee {\nconstructor(el){\nthis.el = el;\nthis.content = el.getAttribute('data-marquee')\nthis.render();\n}\n\n\nrender(){\n\/\/ create the shadow element to measure and calculate\n\/\/ the amount of animated items required for marquee\n\n\/\/ create\nconst shadow = getMarqueeNode(this.content);\nshadow.setAttribute('data-marquee-shadow', true);\n\/\/ add shadow element to sanitized DOM\nthis.el.innerHTML = \"\";\nthis.el.appendChild(shadow);\n\/\/ calculate how many visible items are needed\nconst inView = this.calculateItemsInView(shadow);\n\/\/ create container to house animated visible items\nconst overflow = document.createElement('div');\noverflow.setAttribute('data-marquee-overflow', true);\nconst content = document.createElement('div');\ncontent.setAttribute('data-marquee-container', true);\n\/\/ put the content container into an overflow: hidden wrapper\noverflow.appendChild(content);\n\/\/ double the amount to fill the screen to animate loop\nconst count = inView * 3;\n\/\/ populate with children\nfor( var i = 0; i < count; i++) {\ncontent.appendChild(getMarqueeNode(this.content))\n}\n\/\/ add to DOM\nthis.el.appendChild(overflow);\n\n\/\/ debug\n\/\/ console.log(\"visible items required\", inView);\n\/\/ console.log(\"total items required\", count);\n}\n\ncalculateItemsInView(ref){\nconst [single, total] = [ref.clientWidth, ref.parentNode.clientWidth];\n\/\/ ceil in order to ensure there's never a shortage\nreturn Math.floor(total \/ single) + 1;\n}\n}\n\/\/ find elements and create Marquee instances\nconst nodes = document.querySelectorAll('[data-marquee]')\nconst arr = Array.from(nodes);\nconst refs = (arr || []).map( node => new Marquee(node) );\n\/\/ recalculate on resize\nwindow.addEventListener( 'resize', () => refs.map( r => r.render() ) );\n<\/script>\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t<\/div>\n\t\t\t\t\t\t\t<\/div>\n\t\t<\/section>\n\t\t\t\t<section class=\"elementor-section elementor-top-section elementor-element elementor-element-b09405e elementor-hidden-tablet elementor-hidden-mobile elementor-section-boxed elementor-section-height-default elementor-section-height-default\" data-id=\"b09405e\" data-element_type=\"section\" data-settings=\"{&quot;background_background&quot;:&quot;classic&quot;,&quot;_ha_eqh_enable&quot;:false}\">\n\t\t\t\t\t\t<div class=\"elementor-container elementor-column-gap-default\">\n\t\t\t\t\t<div class=\"elementor-column elementor-col-100 elementor-top-column elementor-element elementor-element-e1167be\" data-id=\"e1167be\" data-element_type=\"column\">\n\t\t\t<div class=\"elementor-widget-wrap elementor-element-populated\">\n\t\t\t\t\t\t\t\t<section class=\"elementor-section elementor-inner-section elementor-element elementor-element-8ed4be3 elementor-section-boxed elementor-section-height-default elementor-section-height-default\" data-id=\"8ed4be3\" data-element_type=\"section\" data-settings=\"{&quot;_ha_eqh_enable&quot;:false}\">\n\t\t\t\t\t\t<div class=\"elementor-container elementor-column-gap-wide\">\n\t\t\t\t\t<div class=\"elementor-column elementor-col-33 elementor-inner-column elementor-element elementor-element-da0353f\" data-id=\"da0353f\" data-element_type=\"column\">\n\t\t\t<div class=\"elementor-widget-wrap elementor-element-populated\">\n\t\t\t\t\t\t\t\t<div class=\"elementor-element elementor-element-5cfd7b8 elementor-align-center elementor-widget elementor-widget-lottie\" data-id=\"5cfd7b8\" data-element_type=\"widget\" data-settings=\"{&quot;source_json&quot;:{&quot;url&quot;:&quot;https:\\\/\\\/techersmart.com.br\\\/wp-content\\\/uploads\\\/2023\\\/10\\\/Lottie-Wifi.json&quot;,&quot;id&quot;:936,&quot;size&quot;:&quot;&quot;,&quot;alt&quot;:&quot;&quot;,&quot;source&quot;:&quot;library&quot;},&quot;loop&quot;:&quot;yes&quot;,&qu